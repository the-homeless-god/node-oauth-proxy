{
  "name": "node-oauth-proxy",
  "version": "0.2.0",
  "description": "Simple server to proxy with oAuth",
  "main": "dist/server.js",
  "scripts": {
    "build": "babel src --out-dir dist --extensions '.ts' --source-maps inline",
    "check": "npm run prettier && npm run eslint",
    "eslint": "eslint --cache --ext .js,.ts, src",
    "eslint:dump": "eslint --print-config ./.eslintrc.json",
    "eslint:fix": "eslint --fix ./*.js ./src/*.ts ./src/**/*.ts --max-warnings=0",
    "fix": "npm run prettier && npm run eslint:fix",
    "prettier": "prettier --write ./src/**",
    "prettier:root": "npm run prettier:root:json && npm run prettier:root:js",
    "prettier:root:js": "prettier --write ./*.js",
    "prettier:root:json": "prettier --write ./*.json",
    "start:development": "npm run build && npm run start:production",
    "start:production": "node dist/server.js",
    "test": "env TS_NODE_COMPILER_OPTIONS='{\"module\": \"commonjs\" }' mocha -r ts-node/register 'src/**/*.spec.ts'",
    "watch": "nodemon --exec npm run start",
    "update:install": "npm update && npm install",
    "update:pull": "npm outdated"
  },
  "author": "Marat Zimnurov <zimtir@mail.ru>",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cookie-parser": "^1.4.5",
    "cors": "^2.8.5",
    "dotenv": "^8.2.0",
    "express-list-endpoints": "^5.0.0",
    "express": "^4.17.1",
    "express-session": "^1.17.1",
    "http": "0.0.1-security",
    "http-proxy": "^1.18.1",
    "passport": "^0.4.1",
    "passport-google-oauth": "^2.0.0"
  },
  "devDependencies": {
    "@babel/cli": "^7.12.10",
    "@babel/core": "^7.12.10",
    "@babel/node": "^7.12.10",
    "@babel/preset-env": "^7.12.11",
    "@babel/preset-typescript": "^7.12.7",
    "@types/chai": "^4.2.14",
    "@types/cookie-parser": "^1.4.2",
    "@types/cors": "^2.8.9",
    "@types/express": "^4.17.11",
    "@types/express-session": "^1.17.3",
    "@types/http-proxy": "^1.17.4",
    "@types/mocha": "^8.2.0",
    "@types/node": "^14.14.20",
    "@types/passport": "^1.0.5",
    "@typescript-eslint/eslint-plugin": "^4.12.0",
    "@typescript-eslint/parser": "^4.12.0",
    "babel-eslint": "^10.1.0",
    "chai": "^4.2.0",
    "eslint": "^7.17.0",
    "eslint-config-airbnb-base": "^14.2.1",
    "eslint-import-resolver-typescript": "^2.3.0",
    "eslint-plugin-babel": "^5.3.1",
    "eslint-plugin-import": "^2.22.1",
    "mocha": "^8.2.1",
    "nodemon": "^2.0.7",
    "prettier": "^2.2.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/Zimtir/node-oauth-proxy.git"
  },
  "homepage": "https://github.com/Zimtir/node-oauth-proxy",
  "keywords": [
    "node",
    "proxy",
    "oauth",
    "reverse-proxy",
    "docker",
    "docker-compose",
    "express",
    "node",
    "typescript"
  ]
}
